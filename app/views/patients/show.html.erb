<h4>Welcome, <%= @patient.fullname %></h4>

<h3><%= link_to "Schedule an Appointment", select_specialty_path %></h3>

<% @patient.doctors.each do |doctor| %>
    <div>
        <strong><%= doctor.fullname %></strong>
        <%= doctor.specialty %>
    </div>
<% end %>

<% @patient.medical_record %>
<% @patient.test_results %>
<% @patient.medications %>



<%= month_calendar(events: @patient.healthcare_teams) do |date, appointments| %>
  <%= date.day %>

  <% appointments.each do |appointment| %>
    <div>
      <%= appointment.doctor.fullname %>
    </div>
  <% end %>
<% end %>