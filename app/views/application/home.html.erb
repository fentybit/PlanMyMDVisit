<div class="center-align">
    <div class="valign-wrapper" style="min-height: 600px; height: 100%;">
        <div class="row" style="width:100%;">
            <% if !logged_in? %>
                <%= link_to "Sign in with GitHub", "/auth/github", class: "waves-effect waves-light btn" %>
                <%= link_to "Sign in with Google", "/auth/google_oauth2", class: "waves-effect waves-light btn" %>
            <% end %>
        </div>
    </div>
</div>